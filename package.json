{
  "name": "searchdocszhCN",
  "displayName": "searchdocszhCN",
  "description": "A simple copy of searchdocs, just replace en-US with zh-CN. Thanks to roblourens and his/her searchdocs",
  "publisher": "winnnntttter",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:searchdocszhCN.searchSO",
    "onCommand:searchdocszhCN.searchMDN"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "searchdocszhCN.searchSO",
        "title": "Search Stack Overflow"
      },
      {
        "command": "searchdocszhCN.searchMDN",
        "title": "Search MDN"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "searchdocs configuration",
      "properties": {
        "searchdocs.searchWordUnderCursor": {
          "type": "boolean",
          "default": false,
          "description": "When there's no selection, search for the word under the cursor, instead of showing an input box."
        }
      }
    },
    "keybindings": [
      {
        "command": "searchdocszhCN.searchSO",
        "key": "ctrl+alt+shift+s",
        "mac": "cmd+alt+shift+s",
        "when": "editorTextFocus"
      },
      {
        "command": "searchdocszhCN.searchMDN",
        "key": "ctrl+alt+shift+m",
        "mac": "cmd+alt+shift+m",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
		"test": "node ./test/runTest.js"
	},
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "eslint": "^6.6.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "tsd": "^0.11.0",
    "typescript": "^3.6.4",
    "vscode": "^1.1.36",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "open": "^7.0.0"
  }
}
